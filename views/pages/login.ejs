<!DOCTYPE html>
<html>
    <head>
        <title>Login</title>
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
        <link rel="stylesheet" href="style.css">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.11.2/css/all.css">
    </head>
    <body>
      <div class="col">
          <div id="inputArea">
              <h3 class = "text-center">Login</h3>
                  <div class="row">
                      <div class="col-3"><p>Email:</p></div>
                      <div class="col"><input type="text" id="email_input" value="email"></div>
                  </div>
                  <div class="row">
                      <div class="col-3"><p>Password</p></div>
                      <div class="col"><input type="password" id="password_input" value="password"></div>
                  </div>
                  <div class="row">
                      <div class="col-2"><button id = "loginButton" type="button" class="btn btn-outline-secondary btn-sm">Login</button></div>
                  </div>
          </div>
      </div> 
    </body>
    <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.4.1.js"
            integrity="sha256-WpOohJOqMqqyKL9FccASB9O0KwACQJpFTUBLTYOVvVU="
            crossorigin="anonymous"></script>
     <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>

    <script type="text/javascript" src="login.js"></script>
</html>
